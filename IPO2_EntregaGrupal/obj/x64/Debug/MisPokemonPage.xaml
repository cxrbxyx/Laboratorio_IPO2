<Page
    x:Class="IPO2_EntregaGrupal.MisPokemonPage"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:IPO2_EntregaGrupal"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:pokemonControls="using:IPO2_EntregaGrupal.PokemonControls"
    mc:Ignorable="d"
    Background="{ThemeResource ApplicationPageBackgroundThemeBrush}">

    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="*" />
            <RowDefinition Height="*" />
        </Grid.RowDefinitions>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="*" />
            <ColumnDefinition Width="*" />
            <ColumnDefinition Width="*" />
        </Grid.ColumnDefinitions>
        <pokemonControls:BulbasaurRH x:ConnectionId='2' x:Name="ucBulbasaur" Grid.Row="0" Grid.Column="0" Margin="10" />
        <pokemonControls:GengarJMC x:ConnectionId='3' x:Name="ucGengar" Grid.Row="0" Grid.Column="1" Margin="10" />
        <pokemonControls:VictiniLDM x:ConnectionId='4' x:Name="ucVictini" Grid.Row="0" Grid.Column="2" Margin="10" />
        <pokemonControls:SwabluSCP x:ConnectionId='5' x:Name="ucSwablu" Grid.Row="1" Grid.Column="0" Margin="10" />
        <pokemonControls:DunsparcePCA x:ConnectionId='6' x:Name="ucDunsparce" Grid.Row="1" Grid.Column="1" Margin="10" />
    </Grid>
</Page>

